lines.linewidth: 0.5               # line width in points
lines.color:     dm.blue4                # has no affect on plot(); see axes.prop_cycle
lines.dashed_pattern: 3, 1.5
lines.dashdot_pattern: 1, 1.25, 4, 1.25
lines.dotted_pattern: 1.0, 1.25

patch.linewidth:       0.5    # edge width in points.

font.family: roboto
#font.style:   normal
#font.variant: normal
font.weight:  300
#font.stretch: normal
font.size:    7.5

mathtext.fontset: custom  # Should be 'dejavusans' (default),

mathtext.bf:  Noto Sans Math:bold
# mathtext.cal: cursive
mathtext.it:  Noto Sans Math:italic
mathtext.rm:  Noto Sans Math
mathtext.sf:  Noto Sans Math
mathtext.tt:  Noto Sans Math
mathtext.fallback: None  # Select fallback font from ['cm' (Computer Modern), 'stix'
                        # 'stixsans'] when a symbol can not be found in one of the
                        # custom math fonts. Select 'None' to not perform fallback
                        # and replace the missing character by a dummy symbol.
mathtext.default: it  # The default font to use for math.
                       # Can be any of the LaTeX font names, including
                       # the special name "regular" for the same font
                       # used in regular text.

axes.linewidth:     0.3     # edge line width
axes.grid:          False   # display grid or not
axes.titlesize:     8.5   # font size of the axes title
axes.titleweight:   400  # font weight of title
axes.labelsize:     7.5  # font size of the x and y labels
axes.labelweight:   400  # weight of the x and y labels
axes.axisbelow:     line    # draw axis gridlines and ticks:
                             #     - below patches (True)
                             #     - above patches but below lines ('line')
                             #     - above all (False)

#axes.spines.left:   True  # display axis spines
#axes.spines.bottom: True
axes.spines.top:    True
axes.spines.right:  True

axes.xmargin:   0.0  # x margin.  See `axes.Axes.margins`
axes.ymargin:   0.0  # y margin.  See `axes.Axes.margins`
axes.zmargin:   0.0  # z margin.  See `axes.Axes.margins`

xtick.major.size:    2     # major tick size in points
xtick.minor.size:    1       # minor tick size in points
xtick.major.width:   0.3     # major tick width in points
xtick.minor.width:   0.20     # minor tick width in points
xtick.major.pad:     2     # distance to major tick label in points
xtick.minor.pad:     2     # distance to the minor tick label in points
#xtick.color:         black   # color of the ticks
#xtick.labelcolor:    inherit # color of the tick labels or inherit from xtick.color
xtick.labelsize:     7  # font size of the tick labels
xtick.direction:     out     # direction: {in, out, inout}
xtick.minor.visible: False   # visibility of minor ticks on x-axis

ytick.major.size:    2     # major tick size in points
ytick.minor.size:    1       # minor tick size in points
ytick.major.width:   0.3     # major tick width in points
ytick.minor.width:   0.20     # minor tick width in points
ytick.major.pad:     1     # distance to major tick label in points
ytick.minor.pad:     1     # distance to the minor tick label in points
#ytick.color:         black   # color of the ticks
#ytick.labelcolor:    inherit # color of the tick labels or inherit from ytick.color
ytick.labelsize:     7  # font size of the tick labels
ytick.direction:     out     # direction: {in, out, inout}
ytick.minor.visible: False   # visibility of minor ticks on y-axis

grid.color:    dm.gray3        # grid color
grid.linestyle: -      # solid
grid.linewidth: 0.3         # in points

legend.frameon:       False     # if True, draw the legend on a background patch
legend.framealpha:    1.0      # legend patch transparency
legend.edgecolor:     0.0      # background patch boundary color
legend.fancybox:      False     # if True, use a rounded box for the
legend.numpoints:     1        # the number of marker points in the legend line
legend.fontsize:      5.5

## Dimensions as fraction of font size:
legend.borderpad:     0.1  # border whitespace
legend.labelspacing:  0.4  # the vertical space between the legend entries
legend.handletextpad: 0.3  # the space between the legend line and legend text
legend.borderaxespad: 0.4  # the border between the axes and legend edge
legend.columnspacing: 0.75  # column separation

figure.titleweight: 400    # weight of the figure title
figure.labelsize:   7.5     # size of the figure label (``Figure.sup[x|y]label()``)
figure.labelweight: 300    # weight of the figure label
figure.figsize:     3.5, 3.0  # figure size in inches
figure.dpi:         300       # figure dots per inch
figure.facecolor:   white     # figure face color

savefig.dpi:       500      # figure dots per inch or 'figure'
savefig.facecolor: white        # figure face color when saving
savefig.bbox:      standard    # {tight, standard}
ps.fonttype:       42       # Output Type 3 (Type3) or Type 42 (TrueType)